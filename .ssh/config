	KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256

Host altiscale
	User schana
	HostName altiscale.com
	Port 1000
	IdentityFile ~/.ssh/id_rsa_altiscale
	Compression yes
	ServerAliveInterval 15
	TCPKeepAlive yes
	DynamicForward localhost:1000
